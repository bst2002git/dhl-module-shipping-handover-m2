<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="print_shipping_handover_label">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">print_shipping_handovernotes</item>
                        <item name="label" xsi:type="string" translate="true">Print Handover Note</item>
                        <item name="url" xsi:type="url" path="dhl_shipping_handover/note/printout"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
